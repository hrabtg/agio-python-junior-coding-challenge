{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "FastAPI",
         "type": "python",
         "request": "launch",
         "module": "uvicorn",
         "args": [
            "src.main:app",
            "--reload",
            "--host",
            "127.0.0.1",
            "--port",
            "8000"
         ],
         "cwd": "${workspaceFolder}",
         "console": "integratedTerminal",
         "justMyCode": true,
      },
      {
         "name": "Local Debug",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "integratedTerminal",
         "justMyCode": true,
      }
   ]
}