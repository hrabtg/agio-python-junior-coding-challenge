{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "FastAPI (Uvicorn)",
         "type": "python",
         "request": "launch",
         "module": "uvicorn",
         "args": [
            "src.main:app",
            "--reload",
            "--host",
            "127.0.0.1",
            "--port",
            "8000"
         ],
         "cwd": "${workspaceFolder}",
         "console": "integratedTerminal",
         "justMyCode": true,
         "python": "${workspaceFolder}/venv/Scripts/python.exe"
      }
   ]
}